{
    "name": "motus-tms-frontend",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate --dotenv .env.dist",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "eslint": "eslint --ignore-pattern .gitignore .",
        "eslintfix": "eslint --fix --ignore-pattern .gitignore .",
        "eslintfix-for-lint-staged": "eslint --fix --max-warnings=0 --ignore-pattern .gitignore",
        "stylelint": "npx stylelint '**/*.{css,scss,vue}'",
        "stylelintfix": "npx stylelint --fix '**/*.{css,scss,vue}'",
        "stylelintfix-for-lint-staged": "npx stylelint --fix --max-warnings=0",
        "typecheck": "npx nuxi typecheck",
        "check-translations": "node ./tools/validateTranslationFiles.js",
        "check-all": "npm run typecheck && npm run eslint && npm run stylelint && npm run check-translations"
    },
    "comments:": [
        "@pinia/nuxt is locked to 0.9.0 only because @pinia-orm/nuxt 1.10.2 requires that",
